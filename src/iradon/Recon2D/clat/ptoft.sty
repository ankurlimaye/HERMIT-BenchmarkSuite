\def\tofigure#1#2#3#4#5#6#7#8
{
  \begin{figure}[htbp]
    \centerline{
      \parbox{#1}
      {
        \epsfxsize #1
        \epsfysize #2
        \epsfbox{#3}
        \caption{\label{#4} #5}
      }
      \hspace*{5mm}
      \parbox{#1}
      {
        \epsfxsize #1
        \epsfysize #2
        \epsfbox{#6}
        \caption{\label{#7} #8}
    }
  }
  \end{figure}
}
